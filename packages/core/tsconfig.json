{
	"include": ["tsdown.config.ts", "src"],
	"compilerOptions": {
		"outDir": "dist",
		"target": "ES2023",
		"lib": ["ES2023", "DOM"],
		"module": "Preserve",
		"moduleResolution": "bundler",
		"types": ["greasemonkey"],
		"allowImportingTsExtensions": true,
		"strict": true,
		"skipLibCheck": true,
		"noUnusedLocals": true,
		"esModuleInterop": true,
		"noEmit": true
	}
}
